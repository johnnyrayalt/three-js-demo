{
  "name": "react-webpack-boilerplate",
  "version": "2.1.0",
  "description": "Minimalistic ES6 React boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./src/**/**.jsx",
    "lintfix": "eslint --fix ./src/**/**.jsx",
    "webpack:dev": "webpack --colors",
    "webpack:prod": "webpack --config webpack.production.config.js --colors"
  },
  "author": "Johnny Ray Alt <johnnyrayalt@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "5.2.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-hot-loader": "4.3.11",
    "react-router-dom": "4.3.0",
    "three": "^0.97.0",
    "three-orbit-controls-loader": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.2.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^2.29.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "uglifyjs-webpack-plugin": "^1.0.0",
    "webpack": "^3.4.0",
    "webpack-dev-server": "^2.5.0"
  }
}

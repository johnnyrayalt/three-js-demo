{
  "name": "react-webpack-boilerplate",
  "version": "2.1.0",
  "description": "Minimalistic ES6 React boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./app/**/**.js",
    "webpack:dev": "webpack --colors",
    "webpack:prod": "webpack --config webpack.production.config.js --colors"
  },
  "author": "Johnny Ray Alt <johnnyrayalt@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "cross-env": "5.2.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-hot-loader": "4.3.11",
    "react-router-dom": "4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.1",
    "@babel/core": "^7.1.1",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-stage-2": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.3",
    "babel-preset-react-hmre": "1.1.1",
    "copy-webpack-plugin": "4.5.2",
    "css-hot-loader": "1.4.2",
    "css-loader": "1.0.0",
    "eslint": "5.0.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.9.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "1.0.1",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.9"
  }
}
